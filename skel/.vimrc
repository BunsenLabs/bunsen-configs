set number
set smartcase
set autoindent
set nowrap
set incsearch
set hlsearch
set cursorline
set whichwrap=b,s,<,>,[,]
set backspace=2
set ruler
set laststatus=2
set noshowcmd
set ttyfast
colo elflord
syntax on
hi CursorLine	cterm=NONE	ctermbg=236	ctermfg=NONE
hi LineNr	cterm=NONE	ctermbg=236	ctermfg=GREY
hi StatusLine	cterm=NONE	ctermbg=236	ctermfg=YELLOW
